{
  "description": "Cmd+Tab → F18 and Cmd+Shift+Tab → F19 (for AeroSpace)",
  "enabled": false,
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "tab",
        "modifiers": { "mandatory": ["command"], "optional": ["any"] }
      },
      "to": [{ "key_code": "f18" }],
      "to_if_alone": [{ "key_code": "f18" }]
    },
    {
      "type": "basic",
      "from": {
        "key_code": "tab",
        "modifiers": { "mandatory": ["command", "shift"], "optional": ["any"] }
      },
      "to": [{ "key_code": "f19" }],
      "to_if_alone": [{ "key_code": "f19" }]
    }
  ]
}
