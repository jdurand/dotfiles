{
  "description": "Disable Cmd+Tab and Cmd+Shift+Tab",
  "enabled": false,
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "tab",
        "modifiers": { "mandatory": ["command"], "optional": ["any"] }
      },
      "to": [{ "key_code": "vk_none" }]
    },
    {
      "type": "basic",
      "from": {
        "key_code": "tab",
        "modifiers": { "mandatory": ["command", "shift"], "optional": ["any"] }
      },
      "to": [{ "key_code": "vk_none" }]
    }
  ]
}
