{
  "description": "Change right_shift to right_shift if pressed with other keys, to return if pressed alone.",
  "enabled": false,
  "manipulators": [
    {
      "from": {
        "key_code": "right_shift",
        "modifiers": { "optional": ["any"] }
      },
      "to": [{ "key_code": "right_shift" }],
      "to_if_alone": [{ "key_code": "return_or_enter" }],
      "type": "basic"
    }
  ]
}
