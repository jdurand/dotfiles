{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "lavender": "#B7BDF8",
    "blue":  "#8AADF4",
    "wight": "#FFFFFF",
    "text": "#494D64"
  },
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "background": "p:blue",
          "foreground": "p:wight",
          "powerline_symbol": "\ue0b4",
          "template": "{{.Icon}} "
        },
        {
          "type": "session",
          "style": "diamond",
          "background": "p:blue",
          "foreground": "p:wight",
          "template": "{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "background": "p:pink",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "template": " {{ .Path }}",
          "properties": {
            "folder_icon": " \ue5fe ",
            "home_icon": "~",
            "style": "agnoster_short"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "type": "rprompt",
      "segments": [
        {
          "type": "git",
          "style": "diamond",
          "background": "p:lavender",
          "foreground": "p:text",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .HEAD }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          }
        },
        {
          "type": "ruby",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#db4b4c",
          "powerline_symbol": "\ue0b4",
          "template": "  {{ .Full }}"
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": "  {{ .Full }} "
        }
      ]
    }
  ],
  "final_space": true,
  "auto_upgrade": true,
  "version": 2
}
